@model List<ITRW324Project2_MVC_.Booking>
@{
    ViewData["Title"] = "Example of a view connected to a DB";
}

<div class="text-center">
    <h1>ASP.NET CORE - MySQL - Entity Framework </h1>
    <div>&nbsp;</div>
    <table class="table table-hover">
        <thead>
            <tr>
                <th scope="col">BookingFrom</th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                if (Model.Count() > 0)
                {
                    foreach (var item in Model)
                    {
            <tr>
                        <td>@item.BookingFrom</td>
            </tr>
                    }
                }
            }
        </tbody>
    </table>
</div>
